<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<bean class="com.liferay.changeset.service.impl.ChangesetLocalServiceImpl" id="com.liferay.changeset.service.ChangesetLocalService" />
	<bean class="com.liferay.changeset.service.persistence.impl.ChangesetPersistenceImpl" id="com.liferay.changeset.service.persistence.ChangesetPersistence" parent="basePersistence" />
	<bean class="com.liferay.changeset.service.impl.ChangesetEntryLocalServiceImpl" id="com.liferay.changeset.service.ChangesetEntryLocalService" />
	<bean class="com.liferay.changeset.service.persistence.impl.ChangesetEntryPersistenceImpl" id="com.liferay.changeset.service.persistence.ChangesetEntryPersistence" parent="basePersistence" />
</beans>